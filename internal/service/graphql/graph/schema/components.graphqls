extend type Query {
  """
  All components running in Alloy.
  """
  components: [Component!]!

  """
  Component by ID.
  """
  component(id: ID!): Component
}

type Component {
  id: ID!
  name: String!
  health: Health!
  arguments: String!
  exports: String!
  debugInfo: String!
  connectionStatus: String!
}

type Health {
  message: String!
  lastUpdated: Time!
}
